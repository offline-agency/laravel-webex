{
  "name": "offline-agency/laravel-webex",
  "description": "A simple integration with Webex",
  "keywords": [
    "offline-agency",
    "integration",
    "api",
    "laravel",
    "webex"
  ],
  "homepage": "https://github.com/offline-agency/laravel-webex",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Offline Agency",
      "email": "support@offlineagency.it",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "ext-json": "*",
    "illuminate/support": "6.0|^7.0|^8.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "illuminate/collections": "6.0|^7.0|^8.0",
    "illuminate/container": "6.0|^7.0|^8.0",
    "illuminate/http": "6.0|^7.0|^8.0"
  },
  "require-dev": {
    "doctrine/dbal": "^2.5",
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^4.0|^6.0",
    "php-coveralls/php-coveralls": "^2.4",
    "phpunit/phpunit": "^6.0|^7.0|^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Offlineagency\\LaravelWebex\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Offlineagency\\LaravelWebex\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Offlineagency\\LaravelWebex\\LaravelWebexServiceProvider"
      ],
      "aliases": {
        "LaravelWebex": "Offlineagency\\LaravelWebex\\LaravelWebexFacade"
      }
    }
  }
}
