{
    "name": "offline-agency/laravel-webex",
    "description": "A simple integration with Webex",
    "keywords": [
        "offline-agency",
        "laravel-webex"
    ],
    "homepage": "https://github.com/offline-agency/laravel-webex",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Offline Agency",
            "email": "support@offlineagency.it",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Offlineagency\\LaravelWebex\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Offlineagency\\LaravelWebex\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Offlineagency\\LaravelWebex\\LaravelWebexServiceProvider"
            ],
            "aliases": {
                "LaravelWebex": "Offlineagency\\LaravelWebex\\LaravelWebexFacade"
            }
        }
    }
}
